    "react-native-collapsible": "brandonfancher/react-native-collapsible",
    "react-native-mapbox-gl": "5.0.0",
    "react-native-quick-actions": "0.1.5",
    "react-native-user-defaults": "0.1.4",
    "react-navigation": "1.0.0-beta.11",
    "react-test-renderer": "15.3.2",
    "styled-components": "2.1.0"
  },
  "jest": {
    "preset": "jest-react-native",
    "globals": {
      "__DEV__": true
    },
    "preprocessorIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)"
    ],
    "coverageReporters": [
      "lcov",
      "json"
    ]
  },
  "devDependencies": {
    "babel-jest": "16.0.0",
